def function_that_prints():
    print ("I printed")

def function_that_returns():
    return "I returned"

f1 = function_that_prints()
f2 = function_that_returns()
print ("Now let us see what the values of f1 and f2 are")
print (f1)
print(f2)

# you can see here that the return statment can be reused but the print function only shows
# the end user what is going on in the computer
